    <string name="voice_studio_move_time">Now it’s time to record the final audio in the Voice Studio phase.</string>

    <!-- Strings from Bloom Library Reader Web Download pages  -->
    <string name="read_now">Read Now</string>
    <string name="downloads_are_complete">Book downloads are complete</string>
    <string name="download_is_complete">Book download is complete</string>
    <string name="view_books">View books</string>
    <string name="downloading_file">Downloading: %s</string>
    <string name="downloading_books">Downloading books</string>
    <string name="book_already_downloaded">The book has already been downloaded for language \'%s\'.</string>
    <string name="del_story_title">Story Deletion</string>
    <string name="del_story_message">Do you want to delete the story \"%1$s\" from your device? Deleting this story will delete any audio recordings you have made in this story.</string>

    <!-- Strings for Help Tutorial views: titles and body  -->
    <string name="help_main_welcome_body">
    <![CDATA[This blue Tutorial window gives you step-by-step guidance to complete your first Bible story video.<br/>Tap <b>Next</b> to begin.]]>
    </string>
    <string name="help_main_drag_title"></string>
    <string name="help_main_drag_body">
        <![CDATA[• You can drag the blue tutorial window out of the way whenever you need to.<br/>• Try it now.]]>
    </string>
    <string name="help_main_help_title">Help Menu</string>
    <string name="help_main_help_body">
        <![CDATA[You can tap on the blue <b>[?]</b> button anytime to get help for the current screen, or to restart the tutorial.
        <br/>Right now, tap Next...]]>
    </string>
    <!--<string name="help_welcome_body3">
        <![CDATA[• You can tap the <b>X</b> to hide the tutorial anytime.
<br/>• Tap the blue help <b>[?]</b> button in the upper right to Resume displaying the tutorial.
<br/>• Try them now.]]>
    </string>
    -->
    <string name="help_main_guidance_title">Guidance</string>
    <string name="help_main_guidance_body">
        <![CDATA[This tutorial will be displayed throughout your first walkthrough of Story Producer.
        <b>Perform each instruction when stated</b> before moving on to the next step.]]>
    </string>
    <string name="help_main_screen_title">Story List</string>
    <string name="help_main_screen_body">
        <![CDATA[This main screen shows a list of the stories currently installed.]]>
    </string>
    <!--<string name="help_main_menus_settings_title"></string>
    <string name="help_main_menus_settings_body">
        <![CDATA[Menu and Settings are available here.]]>
    </string>
    -->
    <string name="help_main_downloads_title">Download Stories</string>
    <string name="help_main_downloads_body">
        <![CDATA[When you are ready for more stories, you can download them using the menu.
        <br/>For now, the \'Demo\' story is great for learning Story Producer. Tap Next…]]>
    </string>
    <string name="help_main_story_title"></string>
    <string name="help_main_story_body">
        <![CDATA[Tap on a story to begin your translation work.]]>
    </string>

    <string name="help_learn_phase_title">Learn Phase</string>
    <string name="help_learn_phase_body">
        <![CDATA[In this first phase, your task is to learn and retell the story.]]>
    </string>
    <string name="help_learn_listen_title">Listen</string>
    <string name="help_learn_listen_body">
        <![CDATA[Play the story several times until you learn the story well enough to retell it.
        <br/><b>Go ahead</b>, try it now.]]>
    </string>
    <string name="help_learn_practice_title">Practice</string>
    <string name="help_learn_practice_body">
        <![CDATA[Practice retelling the story in your local language several times. When you are ready, tap the microphone and start recording it.<br/>(Then tap the square to stop recording.)]]>
    </string>
    <string name="help_learn_listen2_title">Listen</string>
    <string name="help_learn_listen2_body">
        <![CDATA[Tap this button to play back your recorded story. Listen to it now.
        <br/>If you want, you may record it again.]]>
    </string>
    <string name="help_learn_next_phase_title">Next Phase</string>
    <string name="help_learn_next_phase_body">
        <![CDATA[When you finish the Learn Phase, tap here to go to the next phase, Translate + Revise.]]>
    </string>

    <string name="help_translate_phase_title">Translate + Revise Phase</string>
    <string name="help_translate_phase_body">
        <![CDATA[In this phase, your task is to translate each slide into your local language.]]>
    </string>
    <string name="help_translate_titleslide_title">Title Slide</string>
    <string name="help_translate_titleslide_body">
        <![CDATA[Every story begins with a Title Slide, followed by the Story Slides. This label identifies this slide.]]>
    </string>
    <!--
    <string name="help_translate_navigateslides_title">Navigating Slides</string>
    <string name="help_translate_navigateslides_body">
        <![CDATA[Note that you can <b>swipe left or right</b> at any time to move to a different slide. But...
        <br/>Right now, tap Next.]]>
    </string>
    <string name="help_translate_skiptitle_title">Title Slide</string>
    <string name="help_translate_skiptitle_body">
        <![CDATA[Initially it’s best to move ahead and translate the Story Slides first.
        <br/>To begin doing that <b><i>Swipe Left Now</b></i> which will move us to Story Slide 1.
        <br/>&lt;&lt;&lt;&lt;&lt;&lt;
        <br/>If you have already translated all the story slides, tap Next now.]]>
    </string>
    <string name="help_translate_resumetitle_title"></string>
    <string name="help_translate_resumetitle_body">
        <![CDATA[If you have translated the Story Slides, let\'s complete the Title slide now. Tap Next...]]>
    </string>
    -->
    <string name="help_translate_pick_title">Translate a Title</string>
    <string name="help_translate_pick_body">
        <![CDATA[Consider the title ideas below. Pick one and think about how you would translate it into your language.]]>
    </string>
    <string name="help_translate_enter_title">Enter the Title</string>
    <string name="help_translate_enter_body">
        <![CDATA[Tap the pencil now and enter text for your translated story title in your language.]]>
    </string>
    <string name="help_translate_record_title">Record the Title</string>
    <string name="help_translate_record_body">
        <![CDATA[Now tap the microphone to record audio for the title.<br/>(Then tap the square to stop recording.)]]>
    </string>
    <string name="help_translate_nextslide1_title">Swipe Left</string>
    <string name="help_translate_nextslide1_body">
        <![CDATA[When you are done on this Title slide, <b><i>swipe left now</i></b> which will move us to Story Slide 1.
        <br/>&lt;&lt;&lt;&lt;&lt;&lt;]]>
    </string>
    <string name="help_translate_storyslide_title">Story Slide</string>
    <string name="help_translate_storyslide_body">
    <![CDATA[This is a Story Slide. This label identifies the slide number.]]>
    </string>
    <string name="help_translate_listen_title">Listen</string>
    <string name="help_translate_listen_body">
        <![CDATA[Listen to the audio for this slide carefully and think about how you could translate it accurately into your language.
        <br/>Listen to it now.]]>
    </string>
    <string name="help_translate_record_slide_title">Record Your Translation</string>
    <string name="help_translate_record_slide_body">
        <![CDATA[When you are ready, tap the microphone and start recording your translation.<br/>(Then tap the square to stop recording.)]]>
    </string>
    <string name="help_translate_review_title">Review</string>
    <string name="help_translate_review_body">
        <![CDATA[Tap this button to play back your recorded translation. Listen to it now.
        <br/>If you want, you may record it again.]]>
    </string>
    <!--<string name="help_translate_list_title">Recordings List</string>
    <string name="help_translate_list_body">
        <![CDATA[This List Play button can be used if you need to review multiple recordings. Much of the time you don’t need this.]]>
    </string>
    -->
    <string name="help_translate_nextslide2_body">
        <![CDATA[When you are done recording this slide, swipe left to move to the next slide.
        <br/>&lt;&lt;&lt;&lt;&lt;&lt;]]>
    </string>
    <string name="help_translate_continue_title">Translate</string>
    <string name="help_translate_continue_body">
        <![CDATA[Do the Listen, Translate, Record, Review process on each Story Slide.]]>
    </string>
    <string name="help_translate_song_slide_title">Song Slide</string>
    <string name="help_translate_song_slide_body">
        <![CDATA[Now that you have finished translating the story slides, you have the option to create a song to summarize the message of the story.]]>
    </string>
    <string name="help_translate_songlabel_title">Song Slide</string>
    <string name="help_translate_songlabel_body">
    <![CDATA[This label identifies the song slide.]]>
    </string>
    <string name="help_translate_record_song_title">Record Your Song</string>
    <string name="help_translate_record_song_body">
        <![CDATA[Creating a song is optional, though language communities are often excited to do it.
        <br/>You could skip it, but if you are willing to have a little fun and see what Story Producer can do,
        just record a few notes now.]]>
    </string>
    <string name="help_translate_song_title_title">Song Title</string>
    <string name="help_translate_song_title_body">
        <![CDATA[Tap the pencil to enter a song title.]]>
    </string>
    <string name="help_translate_song_camera_title">Camera</string>
    <string name="help_translate_song_camera_body">
        <![CDATA[Or you could add a photo of the singers or of something in your village.]]>
    </string>
    <string name="help_translate_nextphase_title">Next Phase</string>
    <string name="help_translate_nextphase_body">
        <![CDATA[When you are done on this Song slide, tap here (or swipe up) to go to the next phase, Community Work.]]>
    </string>
    <!--
    <string name="help_translate_navigatesongdone_body">
        <![CDATA[Remember, you can <b>swipe left or right</b> at any time to move to a slide you want to work on next.
        <br/>When you are done on this Song slide, swipe left (or right several times) and <b>move to the Title slide</b>.
        <br/>&lt;&lt;&lt;&lt;&lt;&lt; &gt;&gt;&gt;&gt;&gt;&gt;]]>
    </string>
    -->

    <string name="help_community_phase_title">Community Work Phase</string>
    <string name="help_community_phase_body">
        <![CDATA[In this phase, your task is to gather community feedback on your translation.]]>
    </string>
    <string name="help_community_swipe_title"></string>
    <string name="help_community_swipe_body">
        <![CDATA[We need to begin on the Title slide. If a different slide is showing,
        swipe now left or right until the Title slide is showing.
        <br/>&lt;&lt;&lt;&lt;&lt;&lt; &gt;&gt;&gt;&gt;&gt;&gt;]]>
    </string>
    <string name="help_community_play_title">Play Audio</string>
    <string name="help_community_play_body">
        <![CDATA[Allow people in your community to play the translation for this slide.<br/>Is it meaningful?<br/>Are the words clear and natural?]]>
    </string>
    <string name="help_community_record_title"></string>
    <string name="help_community_record_body">
        <![CDATA[Ask people to give feedback directly or by recording their comments using the microphone button.]]>
    </string>
    <string name="help_community_continue_title">Gather Feedback</string>
    <string name="help_community_continue_body">
        <![CDATA[Go to each slide.&ensp;&lt;&lt;&lt;&lt;&lt;&lt;<br/>Gather community feedback for every slide.]]>
    </string>
    <string name="help_community_revise_title">Revise</string>
    <string name="help_community_revise_body">
        <![CDATA[Use the feedback to improve the translation.
        <br/>When needed, <b>go back to the Translate + Revise Phase to make a revision</b>.
        <br/>Then return here to Community Work to continue getting feedback.]]>
    </string>
    <string name="help_community_nextphase_title">Next Phase</string>
    <string name="help_community_nextphase_body">
        <![CDATA[When you are done gathering feedback and making any revisions, tap here (or swipe up) to move on to the next phase, Accuracy Check.]]>
    </string>

    <string name="help_accuracy_phase_title">Accuracy Check Phase</string>
    <string name="help_accuracy_phase_body">
        <![CDATA[In this phase, your task is to have another person check and approve the story.
        <br/>If this is a Bible story, he/she should have Bible knowledge.]]>
    </string>
    <string name="help_accuracy_history_title">History Log</string>
    <string name="help_accuracy_history_body">
        <![CDATA[The accuracy checker may tap here to review the work that has been done on this slide.]]>
    </string>
    <string name="help_accuracy_play_title">Play Audio</string>
    <string name="help_accuracy_play_body">
        <![CDATA[Allow the accuracy checker to play the translation for this slide now.<br/>Is it accurate?<br/>Do the important words communicate the correct meaning?<br/>Are those words used consistently in the story?]]>
    </string>
    <string name="help_accuracy_revise_title">Revise</string>
    <string name="help_accuracy_revise_body">
        <![CDATA[If the accuracy checker feels that a change is best, go back to the Translate + Revise Phase to make the revisions.
        <br/>Then return here to Accuracy Check to continue checking.]]>
    </string>
    <string name="help_accuracy_confirm_title">Confirm</string>
    <string name="help_accuracy_confirm_body">
        <![CDATA[The accuracy checker should tap here if they approve this slide. A green checkmark indicates \"approved\".]]>
    </string>
    <string name="help_accuracy_continue_title">Continue Checking</string>
    <string name="help_accuracy_continue_body">
        <![CDATA[Go to each slide.&ensp;&lt;&lt;&lt;&lt;&lt;&lt;<br/>Check each slide for accuracy. Revise a slide\'s translation if necessary.]]>
    </string>

    <string name="help_voice_phase_title">Voice Studio Phase</string>
    <string name="help_voice_phase_body">
        <![CDATA[In this phase, your task is to re-record the approved translation with a person who has a good story teller voice.<br/>Do this in a quiet place.]]>
    </string>
    <string name="help_voice_listen_title">Listen, Memorize</string>
    <string name="help_voice_listen_body">
        <![CDATA[Practice saying as much of the translation as you can. Tap to pause or resume playback as necessary. Begin now.]]>
    </string>
    <string name="help_voice_record_title">Record Dramatically</string>
    <string name="help_voice_record_body">
        <![CDATA[Re-record the translation with a clear voice and dramatic expression.
        <br/>(Then tap the pause button to pause recording, or tap the square to stop recording.)]]>
    </string>
    <string name="help_voice_replay_title">Replay Recording</string>
    <string name="help_voice_replay_body">
        <![CDATA[Listen to your dramatic re-recording now.
        <br/>If you are not satisfied with the quality, repeat the listening and dramatic re-recording process.]]>
    </string>
    <string name="help_voice_continue_title">Continue</string>
    <string name="help_voice_continue_body">
        <![CDATA[Re-record dramatized audio for each slide.
         <br/>See the detailed help page <b>[?]</b> for additional guidance.]]>
    </string>
    <string name="help_voice_final_title">Final Check</string>
    <string name="help_voice_final_body">
        <![CDATA[Start at the Title slide and double check that you are satisfied with the dramatic audio volume and quality by replaying all of the slides one after the other.
         <br/>Then move on to the Finalize phase.]]>
    </string>
    <string name="help_voice_paused_title">Recording Paused</string>
    <string name="help_voice_paused_body">
        <![CDATA[This special mode can be helpful if you would like to record dramatically in smaller chunks. For example, if you want a different person to speak a character’s voice, or if you need to listen again to a long translation.]]>
    </string>
    <string name="help_voice_pausedreplay_title">Recording Paused</string>
    <string name="help_voice_pausedreplay_body">
        <![CDATA[Tap to hear what has been recorded so far for this slide. Try it now.]]>
    </string>
    <string name="help_voice_pausedlisten_title">Recording Paused</string>
    <string name="help_voice_pausedlisten_body">
        <![CDATA[Tap to resume listening and memorizing the next chunk of the translation.]]>
    </string>
    <string name="help_voice_pausedrecord_title">Resume Recording</string>
    <string name="help_voice_pausedrecord_body">
        <![CDATA[Tap the mic+ button to resume dramatic re-recording.]]>
    </string>
    <string name="help_voice_pausedstop_title">Finalize</string>
    <string name="help_voice_pausedstop_body">
        <![CDATA[Or tap the square stop button to finalize the recording now.]]>
    </string>

    <string name="help_finalize_phase_title">Finalize Phase</string>
    <string name="help_finalize_phase_body">
        <![CDATA[In this phase, your task is to add credits and a short story name, and choose the options for your video.]]>
    </string>
    <string name="help_finalize_credits_title">Credits</string>
    <string name="help_finalize_credits_body">
        <![CDATA[You must enter appropriate credit for all who participated.]]>
    </string>
    <string name="help_finalize_filename_title">Filename</string>
    <string name="help_finalize_filename_body">
        <![CDATA[Type a short story name in your language, for use in the video filename.]]>
    </string>
    <string name="help_finalize_options_title">Video Options</string>
    <string name="help_finalize_options_body">
        <![CDATA[We recommend the default options (Fx), (Px), and (Mv) for your first video.]]>
    </string>
    <!--
    <string name="help_finalize_song_title">Local Song</string>
    <string name="help_finalize_song_body">
        <![CDATA[If you have created a local song and you want it to play at the end of the story video, tick this option.]]>
    </string>
    -->
    <string name="help_finalize_video_title">Create Video</string>
    <string name="help_finalize_video_body">
        <![CDATA[Tap here to create a video with the options selected.]]>
    </string>

    <string name="help_share_phase_title">Share Phase</string>
    <string name="help_share_phase_body">
        <![CDATA[In this last phase, your task is to share your video with others.]]>
    </string>
    <string name="help_share_list_title">Videos Created</string>
    <string name="help_share_list_body">
        <![CDATA[This screen shows a list of the video files you have created.]]>
    </string>
    <string name="help_share_codes_title">Naming Codes</string>
    <string name="help_share_codes_body">
        <![CDATA[The codes, Fx, Sg, Px, Mv, Tx in the filename represent the options you chose in the Finalize phase.]]>
    </string>
    <string name="help_share_ext_title">.mp4 or .3gp?</string>
    <string name="help_share_ext_body">
        <![CDATA[3gp is a low quality video for sharing with feature phones.<br/>Use the mp4 file for smartphones and other devices.]]>
    </string>
    <string name="help_share_play_video_title">Play Video</string>
    <string name="help_share_play_video_body">
        <![CDATA[Watch your video to check it before you share it.]]>
    </string>
    <string name="help_share_share_video_title">Share Video</string>
    <string name="help_share_share_video_body">
        <![CDATA[Tap the Share icon and choose a sharing method.]]>
    </string>
    <string name="help_share_more_title"></string>
    <string name="help_share_more_body">
        <![CDATA[Do you want to create additional video files with different options? Include the song?
        Go back to the Finalize phase and create them.]]>
    </string>
    <string name="help_share_story_list_title">Story List</string>
    <string name="help_share_story_list_body">
        <![CDATA[When you have finished the work on this story, use the menu to return to the Story List.]]>
    </string>

    <string name="help_menu_detailed">Show detailed help for this page</string>
    <string name="help_menu_tutor_restart">Restart Tutorial</string>
    <string name="help_menu_tutor_resume">Resume Tutorial</string>
    <string name="help_tutor_dismiss_message">Tap the blue help <b>[?]</b> button in the toolbar to Resume the Tutorial.</string>
    <string name="help_tutor_no_more_pages">No more tutorial pages for this section.</string>

</resources>
